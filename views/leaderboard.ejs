<div class="container">
  <div class="row">
    <div class="col">
      <table class="table">
        <thead>
          <tr>

            <th scope="col">Place</th>
            <th scope="col">Name</th>
            <th scope="col">Points</th>
          </tr>
        </thead>
        <tbody>


          <% for(var i = 0; i < dataArray.length; i++) { %>


          <tr>
            <th scope="row">
              <%= i + 1%>
            </th>
            <td> <%= dataArray[i].firstName %> <%= dataArray[i].lastName %></td>
            <td> <%= dataArray[i].points %></td>
          </tr>

          <% } %> 


        </tbody>
      </table>

    </div>
  </div>
</div>