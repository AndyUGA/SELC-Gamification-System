    <!-- Accounts Registered Card Component -->
    <div class="col-12 col-md-6">
      <div class="card dashboard-card">
        <div class="row no-gutters pb-2">

          <div class="col-md-7 order-2">

            <div class="account-registered-number">
              <%= dataArray.length %>
            </div>
            <div class="account-registered-title">
              Accounts Registered
            </div>


          </div>
          <div class="col col-md-5 order-1 d-flex justify-content-center align-items-center">
            <img src="./Images/Dashboard/accountRegistered.svg" class="card-img dashboard-card-img" alt="...">
          </div>
        </div>
      </div>
    </div>

    <!-- Edit Teams Card Component -->
    <div class="col-12 col-md-3 my-5 my-md-0">
      <a href="/teamForm">
        <div class="card dashboard-card card-hover">
          <div class="row no-gutters d-flex justify-content-center">
            <div class="col-md-7 order-2 account-registered-title">
              Edit Teams
            </div>
            <div class="col-12 order-1 d-flex justify-content-center align-items-center">
              <img src="./Images/Dashboard/editTeams.svg" class="card-img dashboard-card-img" alt="...">
            </div>
          </div>
        </div>
      </a>
    </div>

    <!-- Edit Points Card Component -->
    <div class="col-12 col-md-3 ">
      <a href="/pointsForm">
        <div class="card dashboard-card card-hover">
          <div class="row no-gutters d-flex justify-content-center">

            <div class="col-md-7 order-2 account-registered-title">
              Edit Points
            </div>
            <div class="col-12 order-1 d-flex justify-content-center align-items-center">
              <img src="./Images/Dashboard/editPoints.svg" class="card-img dashboard-card-img" alt="...">
            </div>
          </div>
        </div>
      </a>
    </div>



  </div>

  <!-- Workshop Table Card Component-->
  <div class="row">

    <div class="col-12 col-md-6 pt-5 ">
      <div class="card workshop-background-color dashboard-card align-items-center">
        <img src="./Images/Dashboard/workshops.svg" class="card-img-top maxIconSize my-3" alt="...">
        <div class="px-5 w-100">
          <h5 class="card-admin-text">
            <div class="row">
              <table class="table table-sm text-UVSASE-blue">
                <thead>
                  <tr>
                    <th class="text-center no-border-top" scope="col">Workshop</th>
                    <th class="text-center no-border-top" scope="col">Registered</th>
                  </tr>
                </thead>
                <tbody class="workshop-card">
                  <% for(var i = 0; i < dataArray2.length; i++) {  %>
                  <tr class="workshopText">
                    <td class="text-center no-border-top">
                      <%= dataArray2[i].name %>
                    </td>
                    <td class="text-center no-border-top">
                      <%= dataArray2[i].attendees.length %> / <%= dataArray2[i].max %>
                    </td>
                  </tr>
                  <% } %>
                </tbody>
              </table>
            </div>
          </h5>
        </div>
      </div>
    </div>
  </div>

  <!-- Table Data for users-->
  <div class="row mt-5">
    <div class="col-12">
      <div class="table-responsive">


        <table class="table">
          <thead class="thead-blue">
            <tr class = "border-color">

              <th scope="col">
                <a class="columnName" href="/?filter=lastName">
                  Name 
                  <img class = "img-fluid filterIcon" src = "./Images/Dashboard/filterIcon.svg"> 
                </a>

              </th>
              <th scope="col">

                <a class="columnName" href="/?filter=email">
                  Email
                  <img class = "img-fluid filterIcon" src = "./Images/Dashboard/filterIcon.svg"> 
                </a>
              </th>
              <th scope="col">

                <a class="columnName" href="/?filter=teamName">
                  Team
                  <img class = "img-fluid filterIcon" src = "./Images/Dashboard/filterIcon.svg"> 
                </a>
              </th>
            </tr>
          </thead>
          <tbody>
            <% for(var i = 0; i < dataArray.length; i++) { %>
            <tr>
              <td> <%= dataArray[i].firstName %> <%= dataArray[i].lastName %></td>
              <td> <%= dataArray[i].email %></td>
              <td> <%= dataArray[i].teamName %></td>
            </tr>
            <% } %>
          </tbody>
        </table>
      </div>
    </div>
  </div>
</div>
</div>