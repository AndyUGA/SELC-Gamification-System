<div class="container">
  <div class="row mt-5">
    <div class="col-12 col-md-6 mb-5 mb-md-0">
      <div class="card dark-blue-bg-color align-items-center">
        <img src="./Images/Dashboard/personIcon2.svg" class="card-img-top maxIconSize mt-3" alt="...">
        <div class="card-body">
          <h5 class="card-admin-text">
            <%= dataArray.length %> Accounts Registered
          </h5>
        </div>
        <div class="d-flex flex-column mb-3">

          <a href="/pointsForm" class="btn btn-UVSASE-darkblue btn-lg mb-3" tabindex="-1" role="button"
            aria-disabled="true">
            Update Points
          </a>

          <a href="/teamForm" class="btn btn-UVSASE-darkblue btn-lg " tabindex="-1" role="button" aria-disabled="true">
            Update Teams
          </a>

        </div>
      </div>
    </div>

    <div class="col-12 col-md-6">
      <div class="card dark-blue-bg-color align-items-center">
        <img src="./Images/Dashboard/workshopIcon.svg" class="card-img-top maxIconSize mt-3" alt="...">
        <div class="card-body w-100">
          <h5 class="card-admin-text">
            <div class="row">
              <table class="table table-sm text-white">
                <thead>
                  <tr>
                    <th class="text-center UVSASE-Yellow" scope="col">Name</th>
                    <th class="text-center UVSASE-Yellow" scope="col">Registered</th>
                  </tr>
                </thead>
                <tbody>
                  <% for(var i = 0; i < dataArray2.length; i++) {  %>
                  <tr class="workshopText">
                    <td class="text-center">
                      <%= dataArray2[i].name %>
                    </td>
                    <td class="text-center">
                      <%= dataArray2[i].attendees.length %> of <%= dataArray2[i].max %>
                    </td>
                  </tr>
                  <% } %>
                </tbody>
              </table>
            </div>
          </h5>
        </div>
      </div>
    </div>
  </div>

  <div class="row mt-5">
    <div class="col">
      <table class="table table-sm">
        <thead class="thead-blue text-white">
          <tr>

            <th scope="col">
              <a class = "columnName" href="/?filter=lastName">
                Name
              </a>

            </th>
            <th scope="col">
              
              <a class = "columnName" href="/?filter=email">
                Email
              </a>
            </th>
            <th scope="col"> 
              
              <a class = "columnName" href="/?filter=teamName">
                Team
              </a>
            </th>
          </tr>
        </thead>
        <tbody>
          <% for(var i = 0; i < dataArray.length; i++) { %>
          <tr>
            <td> <%= dataArray[i].firstName %> <%= dataArray[i].lastName %></td>
            <td> <%= dataArray[i].email %></td>
            <td> <%= dataArray[i].teamName %></td>
          </tr>
          <% } %>
        </tbody>
      </table>
    </div>
  </div>
</div>
</div>